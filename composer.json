{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "version": "6.0.5",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^3.0",
        "bezhansalleh/filament-language-switch": "^3.1",
        "blade-ui-kit/blade-ui-kit": "^0.7.0",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "firebase/php-jwt": "^6.10",
        "gerardojbaez/money": "^0.3.1",
        "guzzlehttp/guzzle": "^7.9",
        "lab404/laravel-impersonate": "^1.7",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.2",
        "league/flysystem-aws-s3-v3": "~3.0",
        "livewire/livewire": "^3.5",
        "lorisleiva/laravel-actions": "^2.8",
        "malzariey/filament-daterangepicker-filter": "^3.0",
        "mariuzzo/laravel-js-localization": "^1.11",
        "opcodesio/log-viewer": "^3.12",
        "owenvoke/blade-fontawesome": "^2.6",
        "pxlrbt/filament-excel": "^2.3",
        "razorpay/razorpay": "^2.9",
        "saade/filament-fullcalendar": "^3.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-google-calendar": "^3.8",
        "spatie/laravel-medialibrary": "^11.8",
        "spatie/laravel-permission": "^6.9",
        "srmklive/paypal": "^3.0",
        "stancl/tenancy": "^3.8",
        "stechstudio/filament-impersonate": "^3.14",
        "stevebauman/location": "*",
        "stripe/stripe-php": "^16.4",
        "tapp/filament-country-code-field": "^1.0",
        "tightenco/ziggy": "^2.4",
        "twilio/sdk": "^8.3",
        "unicodeveloper/laravel-paystack": "^1.2",
        "ysfkaya/filament-phone-input": "^3.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
